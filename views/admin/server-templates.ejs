<script>
  // Fungsi untuk menambah environment variable
  function addEnvVar(type) {
    const container = document.getElementById(type + '-env');
    const newEnvDiv = document.createElement('div');
    newEnvDiv.className = 'flex gap-2';
    newEnvDiv.innerHTML = 
      '<input type="text" name="env_keys[]" placeholder="Key" class="flex-1 px-3 py-2 border border-gray-300 rounded-md focus:outline-none focus:ring-2 focus:ring-' + (type === 'nodejs' ? 'green' : 'blue') + '-500">' +
      '<input type="text" name="env_values[]" placeholder="Value" class="flex-1 px-3 py-2 border border-gray-300 rounded-md focus:outline-none focus:ring-2 focus:ring-' + (type === 'nodejs' ? 'green' : 'blue') + '-500">' +
      '<button type="button" onclick="removeEnvVar(this)" class="px-3 py-2 bg-red-500 text-white rounded-md hover:bg-red-600">‚úï</button>';
    container.appendChild(newEnvDiv);
  }

  // Fungsi untuk menghapus environment variable
  function removeEnvVar(button) {
    button.parentElement.remove();
  }

  // Fungsi untuk test template
  async function testTemplate(type) {
    if (!confirm('Apakah Anda yakin ingin menguji template ' + type + '? Ini akan membuat server test.')) {
      return;
    }

    try {
      const response = await fetch('/admin/template/' + type + '/test', {
        method: 'POST',
        headers: {
          'Content-Type': 'application/json'
        }
      });

      const result = await response.json();

      if (result.success) {
        alert('Template ' + type + ' berhasil diuji! Server test telah dibuat: ' + result.serverId);
      } else {
        alert('Error testing template: ' + result.message);
      }
    } catch (error) {
      alert('Error: ' + error.message);
    }
  }

  // Auto-save on form change (optional)
  document.querySelectorAll('form').forEach(form => {
    const inputs = form.querySelectorAll('input');
    inputs.forEach(input => {
      input.addEventListener('change', function() {
        // Optional: Add unsaved changes indicator
        const submitBtn = form.querySelector('button[type="submit"]');
        if (submitBtn) {
          submitBtn.textContent = submitBtn.textContent.replace('üíæ', '‚ö†Ô∏è');
          submitBtn.classList.add('bg-yellow-600', 'hover:bg-yellow-700');
          submitBtn.classList.remove('bg-green-600', 'hover:bg-green-700', 'bg-blue-600', 'hover:bg-blue-700');
        }
      });
    });
  });
</script>

