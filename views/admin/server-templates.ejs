    if (!confirm(`Apakah Anda yakin ingin menguji template ${type}? Ini akan membuat server test.`)) {
      return;
    }

    try {
      const response = await fetch(`/admin/template/${type}/test`, {
        method: 'POST',
        headers: {
          'Content-Type': 'application/json'
        }
      });

      const result = await response.json();

      if (result.success) {
        alert(`Template ${type} berhasil diuji! Server test telah dibuat: ${result.serverId}`);
      } else {
        alert(`Error testing template: ${result.message}`);
      }
    } catch (error) {
      alert(`Error: ${error.message}`);
    }
  }

