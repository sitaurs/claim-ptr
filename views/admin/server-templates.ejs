              type="text" 
              name="docker_image" 
              value="<%= (config.server_templates && config.server_templates.nodejs && config.server_templates.nodejs.docker_image) || 'ghcr.io/pterodactyl/yolks:nodejs_18' %>" 
              class="w-full px-3 py-2 border border-gray-300 rounded-md focus:outline-none focus:ring-2 focus:ring-green-500"
              required
            >
